{
  "name": "Cyberpunk 2077",
  "colors": {
    // ===== EDITOR =====
    "editor.background": "#272932",
    "editor.foreground": "#FFFFFF",
    "editor.lineHighlightBackground": "#2F3140",
    "editor.selectionBackground": "#9370DB40",
    "editor.selectionHighlightBackground": "#37EBF320",
    "editor.wordHighlightBackground": "#1AC5B030",
    "editor.findMatchBackground": "#FDF50040",
    "editor.findMatchHighlightBackground": "#FDF50020",
    "editorCursor.foreground": "#37EBF3",
    "editorWhitespace.foreground": "#3B3D4F",
    "editorIndentGuide.background1": "#3B3D4F",
    "editorIndentGuide.activeBackground1": "#9370DB80",
    "editorLineNumber.foreground": "#5A5D70",
    "editorLineNumber.activeForeground": "#37EBF3",
    "editorBracketMatch.background": "#1AC5B020",
    "editorBracketMatch.border": "#1AC5B0",
    "editorError.foreground": "#710000",
    "editorWarning.foreground": "#FDF500",

    // ===== ACTIVITY BAR (thanh icon bên trái) =====
    "activityBar.background": "#1C1E28",
    "activityBar.foreground": "#37EBF3",
    "activityBar.inactiveForeground": "#5A5D70",
    "activityBarBadge.background": "#CB1DCD",
    "activityBarBadge.foreground": "#FFFFFF",
    "activityBar.activeBorder": "#37EBF3",

    // ===== SIDEBAR =====
    "sideBar.background": "#21232E",
    "sideBar.foreground": "#D1C5C0",
    "sideBarTitle.foreground": "#37EBF3",
    "sideBarSectionHeader.background": "#1C1E28",
    "sideBarSectionHeader.foreground": "#E455AE",

    // ===== TITLE BAR =====
    "titleBar.activeBackground": "#1C1E28",
    "titleBar.activeForeground": "#D1C5C0",
    "titleBar.inactiveBackground": "#1C1E28",
    "titleBar.inactiveForeground": "#5A5D70",

    // ===== TABS =====
    "tab.activeBackground": "#272932",
    "tab.activeForeground": "#37EBF3",
    "tab.inactiveBackground": "#1C1E28",
    "tab.inactiveForeground": "#5A5D70",
    "tab.activeBorderTop": "#CB1DCD",
    "tab.border": "#1C1E28",
    "tab.hoverBackground": "#2F3140",

    // ===== STATUS BAR (thanh dưới cùng) =====
    "statusBar.background": "#1C1E28",
    "statusBar.foreground": "#37EBF3",
    "statusBar.debuggingBackground": "#710000",
    "statusBar.debuggingForeground": "#D1C5C0",
    "statusBar.noFolderBackground": "#272932",

    // ===== TERMINAL =====
    "terminal.background": "#272932",
    "terminal.foreground": "#D1C5C0",
    "terminal.ansiBlack": "#272932",
    "terminal.ansiRed": "#710000",
    "terminal.ansiGreen": "#1AC5B0",
    "terminal.ansiYellow": "#FDF500",
    "terminal.ansiBlue": "#9370DB",
    "terminal.ansiMagenta": "#CB1DCD",
    "terminal.ansiCyan": "#37EBF3",
    "terminal.ansiWhite": "#D1C5C0",
    "terminal.ansiBrightRed": "#E455AE",
    "terminal.ansiBrightGreen": "#1AC5B0",
    "terminal.ansiBrightYellow": "#FDF500",
    "terminal.ansiBrightBlue": "#9370DB",
    "terminal.ansiBrightMagenta": "#CB1DCD",
    "terminal.ansiBrightCyan": "#37EBF3",
    "terminal.ansiBrightWhite": "#FFFFFF",

    // ===== INPUT & DROPDOWN =====
    "input.background": "#1C1E28",
    "input.foreground": "#D1C5C0",
    "input.border": "#3B3D4F",
    "input.placeholderForeground": "#5A5D70",
    "inputOption.activeBorder": "#37EBF3",
    "dropdown.background": "#1C1E28",
    "dropdown.foreground": "#D1C5C0",
    "dropdown.border": "#3B3D4F",

    // ===== BUTTON =====
    "button.background": "#CB1DCD",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#9370DB",

    // ===== LIST (file explorer, search results) =====
    "list.activeSelectionBackground": "#9370DB30",
    "list.activeSelectionForeground": "#37EBF3",
    "list.hoverBackground": "#2F3140",
    "list.hoverForeground": "#D1C5C0",
    "list.focusBackground": "#9370DB20",
    "list.highlightForeground": "#FDF500",

    // ===== SCROLLBAR =====
    "scrollbar.shadow": "#00000050",
    "scrollbarSlider.background": "#3B3D4F50",
    "scrollbarSlider.hoverBackground": "#9370DB50",
    "scrollbarSlider.activeBackground": "#37EBF350",

    // ===== GIT COLORS =====
    "gitDecoration.modifiedResourceForeground": "#FDF500",
    "gitDecoration.deletedResourceForeground": "#E455AE",
    "gitDecoration.untrackedResourceForeground": "#1AC5B0",
    "gitDecoration.conflictingResourceForeground": "#CB1DCD",

    // ===== PEEK VIEW =====
    "peekView.border": "#37EBF3",
    "peekViewTitle.background": "#1C1E28",
    "peekViewResult.background": "#21232E",
    "peekViewEditor.background": "#272932",

    // ===== MINIMAP =====
    "minimap.selectionHighlight": "#9370DB60",
    "minimap.findMatchHighlight": "#FDF50060",

    // ===== BREADCRUMB =====
    "breadcrumb.foreground": "#5A5D70",
    "breadcrumb.focusForeground": "#37EBF3",
    "breadcrumb.activeSelectionForeground": "#D1C5C0",

    // ===== PANEL (terminal panel, output) =====
    "panel.background": "#272932",
    "panel.border": "#3B3D4F",
    "panelTitle.activeBorder": "#CB1DCD",
    "panelTitle.activeForeground": "#37EBF3",
    "panelTitle.inactiveForeground": "#5A5D70",

    // ===== BADGE =====
    "badge.background": "#CB1DCD",
    "badge.foreground": "#FFFFFF",

    // ===== FOCUS BORDER =====
    "focusBorder": "#37EBF380"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00ff9f"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#D1C5C0"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#710000"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9370DB"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#1AC5B0"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#CB1DCD"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#37EBF3"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#E455AE"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#E455AE"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#E455AE"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#FDF500"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#1AC5B0"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#1AC5B0"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#D1C5C0"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#E455AE"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#CB1DCD"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#37EBF3"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#37EBF3"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#9370DB"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FDF500"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FDF500"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#37EBF3"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#1AC5B0"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#710000"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#9370DB"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#37EBF3"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#1AC5B0"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#37EBF3"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E455AE"
      }
    },
